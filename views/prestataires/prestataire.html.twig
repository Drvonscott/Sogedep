{% extends "layout.html.twig" %}

{% block title %}Prestataire{% endblock %}

{% block body %}

<h2>{{ prestataire.prenom }} {{ prestataire.nom }}</h2>
 <table class="table table-hover">
  <tbody>
   <tr><td>Adresse 1</td><td>{{ prestataire.adresse1 }}</td></tr>
   <tr><td>Adresse 2</td><td>{{ prestataire.adresse2 }}</td></tr>
   <tr><td>Adresse 3</td><td>{{ prestataire.adresse3 }}</td></tr>
   <tr><td>Code postal</td><td>{{ prestataire.codepostal }}</td></tr>
   <tr><td>Ville</td><td>{{ prestataire.ville }}</td></tr>
   <tr><td>Email</td><td>{{ prestataire.email }}</td></tr>
   <tr><td>Initiales</td><td>{{ prestataire.initiales }}</td></tr>
   <tr><td>Pseudo</td><td>{{ prestataire.pseudo }}</td></tr>
   <tr><td>Password</td><td>{{ prestataire.password }}</td></tr>
   <tr><td>Salt</td><td>{{ prestataire.salt }}</td></tr>
   <tr><td>Role</td><td>{{ prestataire.role }}</td></tr>
  </tbody>
 </table> 
 
{% endblock %}