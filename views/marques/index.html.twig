{% extends "layout.html.twig" %}

{% block title %}Marques{% endblock %}

{% block body %}
  
  <h2>Marques</h2>
  <table class="table table-hover">
   <thead>
    <tr>
     <th>Id</th>
     <th>Marque</th>
    </tr>
   </thead>
   <tbody>
   {% for marque in marques %}
    <tr>
     <td>{{ marque.marque }}</td> 
     <td>{{ marque.id }}</td>
    </tr>
   {% endfor %}
   </tbody>
  </table>
 
 {% endblock %}